{
  "Rules": [
    {
      "ID": "DeleteRawAfter14Days",
      "Filter": {
        "And": {
          "Prefix": "job-cards/",
          "Tags": [
            {
              "Key": "type",
              "Value": "raw"
            }
          ]
        }
      },
      "Status": "Enabled",
      "Expiration": {
        "Days": 14
      }
    },
    {
      "ID": "KeepFinishedFilesLonger",
      "Filter": {
        "And": {
          "Prefix": "job-cards/",
          "Tags": [
            {
              "Key": "type",
              "Value": "finished"
            }
          ]
        }
      },
      "Status": "Enabled",
      "Transitions": [
        {
          "Days": 90,
          "StorageClass": "STANDARD_IA"
        },
        {
          "Days": 365,
          "StorageClass": "GLACIER"
        }
      ]
    }
  ]
}